 //   printf("%d\n%d\n",num,i);

        printf("\n1.Table of 1\n2.Table of 2\n3.Table of 3\n4.Table of 4\n5.Table of 5\n6.Exit\n");

    scanf("%d",&num);

        

     if (num<6)

   

     do {   

        printf("%d ",num*i);

        i++;

        

       

    }

      while(i<=10);

    

    

        if (num == 6){

        exit(0); }

    else if (num >6){

