//Write a C program to find the minimum and maximum element of an array.

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int a[1000],i,n,max,min;
      scanf("%d",&n);
    for(i=0; i<n; i++)
        {
        scanf("%d",&a[i]);
        }
     min=max=a[0];
    for(i=1; i<n; i++)
        {
        if(min>a[i])
         min=a[i];
        if(max<a[i])
         max=a[i];
        }
    printf("%d",min);
    printf("\n%d",max);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
