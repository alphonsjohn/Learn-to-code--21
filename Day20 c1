//Write a C program to find the factorial of a given number using a recursive function

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
    int recurse(int n) {
  if (n >=1)  

     return(n*recurse(n-1));  

  else  
      return 1; }
    int main()  

   {  

  int n;  

 
  scanf("%d", &n);   

   
  if (n<0)
      printf("ERROR");
   else 
       printf("%d", recurse(n));
    return 0;

}
