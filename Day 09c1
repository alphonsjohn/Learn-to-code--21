//C program to print the HCF of two numbers

#include <stdio.h>

#include <string.h>

#include <math.h>

#include <stdlib.h>

int main() {

    int n1,n2;

    scanf("%d%d",&n1,&n2);

    while( n1!=n2)

   {

        if( n1>n2)

            n1-=n2;

        else 

            n2-=n1;

        }

        printf("%d",n1);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    

    return 0;

}
